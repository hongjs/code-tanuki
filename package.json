{
  "name": "code-tanuki",
  "version": "1.0.0",
  "description": "AI-powered PR review tool",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docker:build": "docker build -t code-tanuki:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env -v ./data:/data -v ./logs:/logs code-tanuki:latest",
    "docker:compose:up": "docker compose up -d"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@mui/x-data-grid": "^8.27.0",
    "@octokit/rest": "^21.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "axios": "^1.13.5",
    "date-fns": "^4.1.0",
    "next": "16.1.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-next": "^16.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hongjs/code-tanuki.git"
  },
